{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "CodeLens (Launch)",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "args": "",
            "projectName": "temp-workfile"
        },
        {
            "type": "java",
            "name": "Launch in Terminal mode and debug mode",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "stopOnEntry": true
        },
        {
            "type": "java",
            "name": "Launch in Terminal mode",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopOnEntry": false,
        },
        {
            "type": "java",
            "name": "Launch in Powershell",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "externalTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopOnEntry": false,
        },
        {
            "type": "java",
            "name": "Debug (Launch)",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "mainClass": "yeet.Test",
            "args": ""
        }
    ]
}